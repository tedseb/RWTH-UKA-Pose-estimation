<launch>
  <arg name="args" default=" " doc="Parameters of Station Manager e.g: args:='-d'"/>
  <arg name="sms" default="log" doc="Output of station manager, sms:='screen'"/>
  <arg name="ai" default="metrabs" doc="Choose AI out of the following: (metrabs, spin)"/>
  <include file="$(find infrastructure)/launch/station_manager.launch">
    <arg name="args" value="$(arg args)"/>
    <arg name="sms" value="$(arg sms)"/>
  </include>
  <include file="$(find infrastructure)/launch/mobile_server.launch" />
  <include file="$(find metrabs)/launch/metrabs.launch"/>
  <include file="$(find object_detection)/launch/object_detection.launch" />
  <include file="$(find motion_analysis)/launch/motion_analysis.launch">
    <!-- <arg name="ai" value="$(arg ai)"/> -->
  </include>
  <include file="$(find backend)/launch/SkeletonVisualizationHelper.launch">
    <arg name="ai" value="$(arg ai)"/>
  </include>
</launch> 
 

 
